{
  "name": "node-live-stream-recorder",
  "version": "0.2.0",
  "description": "Record live streams from YouTube, Twitch, etc.",
  "dependencies": {
    "@redux-devtools/extension": "^3.2.2",
    "@types/cors": "^2.8.12",
    "@types/lodash": "^4.14.182",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^12.20.55",
    "@types/node-fetch": "^2.6.2",
    "@types/react": "^17.0.47",
    "@types/react-dom": "^17.0.17",
    "@types/react-redux": "^7.1.24",
    "autoprefixer": "^10.4.7",
    "classnames": "^2.3.1",
    "concurrently": "^7.1.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "date-fns": "^2.16.1",
    "dotenv": "^16.0.0",
    "express": "^4.17.1",
    "html-webpack-plugin": "^5.5.0",
    "lodash": "^4.17.20",
    "lodash-es": "^4.17.21",
    "mini-css-extract-plugin": "^2.6.0",
    "nanoid": "^3.1.25",
    "node-fetch": "^2.6.1",
    "normalize.css": "^8.0.1",
    "playwright-core": "^1.20.2",
    "postcss": "^8.4.14",
    "postcss-loader": "^7.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.8",
    "redux": "~4.1.2",
    "redux-thunk": "^2.4.1",
    "sanitize-filename": "^1.6.3",
    "sass": "^1.51.0",
    "sass-loader": "^13.0.0",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.3.0",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.6.11",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.4",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2"
  },
  "scripts": {
    "build": "npm run build:client && npm run build:server",
    "build:client": "cd client && cross-env NODE_ENV=production webpack",
    "build:server": "cd server && tsc && tsc-alias",
    "dev": "concurrently -n server,web -c green,cyan 'npm:start:dev-server' 'npm:start:dev-client'",
    "start": "npm run build && npm run start:server",
    "start:dev-client": "cd client && webpack serve --mode development --port 3000",
    "start:dev-server": "cd server && ts-node-dev --respawn -r tsconfig-paths/register src",
    "start:server": "node server/build"
  },
  "author": "Vyacheslav Abushkevich <abshkvch@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "webpack-dev-server": "^4.8.1"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "repository": "github:vabushkevich/node-live-stream-recorder"
}
