{
  "name": "node-live-stream-recorder",
  "version": "0.1.2",
  "description": "Record live streams from YouTube, Twitch, etc.",
  "dependencies": {
    "@types/react": "^17.0.47",
    "autoprefixer": "^10.4.7",
    "classnames": "^2.3.1",
    "concurrently": "^7.1.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "date-fns": "^2.16.1",
    "dotenv": "^16.0.0",
    "express": "^4.17.1",
    "html-webpack-plugin": "^5.5.0",
    "lodash": "^4.17.20",
    "lodash-es": "^4.17.21",
    "mini-css-extract-plugin": "^2.6.0",
    "nanoid": "^3.1.25",
    "node-fetch": "^2.6.1",
    "normalize.css": "^8.0.1",
    "playwright-core": "^1.20.2",
    "postcss": "^8.4.14",
    "postcss-loader": "^7.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "sanitize-filename": "^1.6.3",
    "sass": "^1.51.0",
    "sass-loader": "^13.0.0",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.3.0",
    "typescript": "^4.7.4",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2"
  },
  "scripts": {
    "build": "cd client && cross-env NODE_ENV=production webpack",
    "dev": "concurrently -n server,web -c green,cyan 'npm:start:dev-server' 'npm:start:dev-client'",
    "start": "npm run build && npm run start:server",
    "start:dev-client": "cd client && webpack serve --mode development --port 3000",
    "start:dev-server": "nodemon server -w server",
    "start:server": "node server"
  },
  "author": "Vyacheslav Abushkevich <abshkvch@gmail.com>",
  "license": "MIT",
  "workspaces": [
    "./server"
  ],
  "devDependencies": {
    "nodemon": "^2.0.15",
    "webpack-dev-server": "^4.8.1"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "repository": "github:vabushkevich/node-live-stream-recorder"
}
